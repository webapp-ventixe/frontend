{
  "navigationFallback": {
    "rewrite": "/index.html",
    "exclude": [
      "/*.{png,jpg,gif,svg,css,js}",
      "/icons/*.{png,jpg,gif,svg}",
      "/logos/*.{png,jpg,gif,svg}"
    ]
  },
  "routes": [
    {
      "route": "/*",
      "serve": "/index.html",
      "statusCode": 200
    }
  ]
}
